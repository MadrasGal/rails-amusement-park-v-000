
<%= form_for @user, :url => url_for(controller: 'sessions', action: 'create') do |f| %>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <%= f.collection_select :user_name, User.all, :id, :user_name %>
    
    <%= f.label :password %>
    <%= f.text_field :password%>
    
    <%= f.submit  "Sign In"%>
<% end %>